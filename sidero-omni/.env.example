# Example environment variables for Omni
# Copy this file to .env and fill in your values

# Omni Version
OMNI_VERSION=1.2.1

# Account Configuration
OMNI_ACCOUNT_UUID=a0fa88a3-638f-43b3-85e1-7bd8c17a1d8c

# Domain Configuration
OMNI_DOMAIN_NAME=omni.example.com

# WireGuard Configuration
OMNI_WG_IP=10.10.1.100

# Admin Email
OMNI_ADMIN_EMAIL=admin@example.com

# Auth0 Configuration
AUTH0_CLIENT_ID=your-auth0-client-id
AUTH0_DOMAIN=dev-xxxxxxxx.us.auth0.com
